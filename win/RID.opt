#
#  gdal.netcore package automation
# 
#    Windows Runtime Identifier 
#
BASE = $(MAKEDIR)
BASE_RUNTIME_RID = win
TARGET_OS = $(BASE_RUNTIME_RID)
BUILD_ARCH = x64
VCPKG_RID = $(BUILD_ARCH)-windows

# .NET RID
RID = $(BASE_RUNTIME_RID)-$(BUILD_ARCH)
SWIG = swig

COPY = copy 
COPY_I = xcopy /s /y /r /d /f /i
COPY_TOP_DIR=xcopy /i /y

DEL_FILE = del
DEL_DIR = del /q /s
CRT_VERSION = 143