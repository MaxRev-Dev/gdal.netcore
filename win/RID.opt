#
#  gdal.netcore package automation
# 
#    Windows Runtime Identifier 
#
BASE=$(MAKEDIR)
BASE_RUNTIME_RID=win
VCPKG_RID=x64-windows

# .NET RID
RID=win-x64
SWIG = "$(BASE)/swigwin-3.0.12/swig.exe"

COPY = xcopy /y /r /d /f
COPY_I = xcopy /s /y /r /d /f /i

DEL_FILE = -del
DEL_DIR = -del /q /s
CRT_VERSION = 143